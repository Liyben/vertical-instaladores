<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.ui.view" id="view_account_analytic_line_form">
        <field name="name">Add cost unit</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="analytic.view_account_analytic_line_form"/>
        <field name="arch" type="xml">
            <group name="analytic_entry" position="after">
                <group/> 
                <group name="cost_produced_unit" string="Unidades Producidas">
                    <field name="product_produced_unit_id" />
                    <field name="produced_unit"/>
                    <field name="cost_produced_unit" widget="monetary"/>
                    <field name="total_cost_produced_unit" widget="monetary"/>
                </group>
            </group>
        </field>
    </record>
    
</odoo>