<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="sequence_notice" model="ir.sequence">
        <field name="name">Notice Code</field>
        <field name="code">notice.code</field>
        <field eval="4" name="padding" />
        <field name="prefix">AV-</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_opportunity" model="ir.sequence">
        <field name="name">Opportunity Code</field>
        <field name="code">opportunity.code</field>
        <field eval="4" name="padding" />
        <field name="prefix">OP-</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
